{
  "name": "ms-expense",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "author": "Bruno Uemura",
  "scripts": {
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "dev:migration:run": "cross-env NODE_ENV=development node --require ts-node/register ./node_modules/typeorm/cli.js migration:run",
    "dev:migration:create": "cross-env NODE_ENV=development node --require ts-node/register ./node_modules/typeorm/cli.js migration:create -n",
    "dev": "cross-env NODE_ENV=development ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "build": "tsc",
    "migration:run": "cross-env NODE_ENV=production node --require ts-node/register ./node_modules/typeorm/cli.js migration:run",
    "migration:create": "cross-env NODE_ENV=production node --require ts-node/register ./node_modules/typeorm/cli.js migration:create -n",
    "start": "cross-env NODE_ENV=production node dist/src/server.js"
  },
  "dependencies": {
    "amqplib": "^0.8.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^12.0.3",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.41",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.10.1",
    "@types/redis": "^2.8.32",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  }
